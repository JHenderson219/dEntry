{
  "manifest_version": 2,

  "name": "dEntry",
  "description": "Data Entry Assistant",
  "version": "0.01",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "dEntry"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}